<?xml version="1.0" encoding="UTF-8"?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="TsuraiClient.Views.iOSRootTabbedPage"
		xmlns:local="clr-namespace:TsuraiClient.Views;assembly=TsuraiClient">
	<NavigationPage Title="つらさ診断">
		<NavigationPage.Icon>
			<OnPlatform x:TypeArguments="FileImageSource" iOS="icon-check-tsurai" />
		</NavigationPage.Icon>
		<x:Arguments>
			<local:TsuraiCheckPage />
		</x:Arguments>
	</NavigationPage>
	<NavigationPage Title="つらさ統計">
		<NavigationPage.Icon>
			<OnPlatform x:TypeArguments="FileImageSource" iOS="icon-analytics" />
		</NavigationPage.Icon>
		<x:Arguments>
			<local:UserStatusAnalyticsPage/>
		</x:Arguments>
	</NavigationPage>
</TabbedPage>
